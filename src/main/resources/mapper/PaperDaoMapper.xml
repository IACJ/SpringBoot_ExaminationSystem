<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="examination.dao.PaperDao">
    <insert id="add">
        INSERT INTO paper(name,begintime,finishtime,choi,judg,sub,choiscore,judgscore,subscore,tid,classid)
        VALUE(#{name},#{begintime},#{finishtime},#{choi},#{judg},#{sub},,#{choiscore},#{judgscore},#{subscore},#{tid},#{classid});
    </insert>

</mapper>